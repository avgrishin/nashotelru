@model IEnumerable<Nashotelru.Models.NewsViewModel>
@using Nashotelru.Resources.Shared
<div class="panel panel-success">
  <div class="panel-heading text-center">@Menu.News</div>
  <div class="panel-body">
    <ul class="list-unstyled">
      @foreach (var item in Model)
      {
        <li><p><small>@Html.DisplayFor(p => item.Date)</small><br />@Html.ActionLink(item.Title, "NewsDetail", new { id = item.ID }, null)</p></li>
      }
    </ul>
  </div>
</div>
